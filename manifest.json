{
  "manifest_version": 3,
  "name": "Wiremock recorder",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "debugger",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "service-worker.js"
  }
}
